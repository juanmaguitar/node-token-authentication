{
  "name": "node-token-authentication",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_PATH=. DEBUG=bin:www nodemon ./",
    "start": "node ./",
    "build:css": "node-sass --include-path scss src/scss/main.scss public/main.css",
    "build:js": "browserify src/js/app -o public/bundle.js -t babelify brfs",
    "watch:css": "nodemon -e scss -x 'npm run build:css'",
    "watch:js": "watchify src/js/app -t brfs -d -o public/bundle.js -v",
    "watch:js:es2015": "watchify src/js/app -t brfs -t babelify -o public/bundle.js -v",
    "test": "mocha test",
    "test:watch": "DEBUG=test:utils nodemon --exec 'clear && npm test'"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-route": "^1.5.8",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "^4.13.4",
    "express-jwt": "~0.2.1",
    "express-validator": "^2.20.8",
    "jade": "~1.11.0",
    "jsonwebtoken": "^5.7.0",
    "mongoose": "^4.5.10",
    "morgan": "^1.7.0",
    "serve-favicon": "~2.3.0",
    "validator": "^5.5.0"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "bootstrap-sass": "^3.3.7",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "browserify-ng-html2js": "^1.3.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "mocha": "^3.0.2",
    "napa": "^2.3.0",
    "nodemon": "^1.10.2",
    "should": "^11.1.0",
    "supertest": "^2.0.0",
    "watchify": "^3.7.0"
  }
}
